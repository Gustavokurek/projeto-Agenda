<% if(errors.length > 0) { %> 
  <div class="row">
    <div class="col">
        <div class="alert-danger"> <% errors.forEach(erro=>{ %>
              <%-erro %> <br>
        <% }) %>  </div>
    </div>
  </div>
  <% } %> 
  
  <% if(success) { %> 
    <div class="row">
      <div class="col">
          <div class="alert-success"> <% success.forEach(success=>{ %>
                <%-success %> <br>
          <% }) %>  </div>
      </div>
    </div>
    <% } %> 

